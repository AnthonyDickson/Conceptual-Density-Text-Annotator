{
  "name": "annotator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.22.0",
    "body-parser": "^1.18.3",
    "dotenv": "^8.1.0",
    "express": "^4.16.4",
    "mysql": "^2.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "react-text-annotate": "github:eight0153/react-text-annotate",
    "react-timeago": "^4.4.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1",
    "concurrently": "^4.0.1"
  },
  "scripts": {
    "client": "cd client && npm run start",
    "server": "nodemon server/index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "start": "node server/index.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
